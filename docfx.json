          "src": "../../"
        }
      ],
      "dest": "api/Heleonix.Execution"
    },
	{
      "src": [
        {
          "files": [ "Heleonix.Guard/bin/Release/netstandard2.0/Heleonix.Guard.dll" ],
          "src": "../../"
        }
      ],
      "dest": "api/Heleonix.Guard"
    },
	{
      "src": [
        {
          "files": [ "Heleonix.Reflection/bin/Release/netstandard2.0/Heleonix.Reflection.dll" ],
          "src": "../../"
        }
      ],
      "dest": "api/Heleonix.Reflection"
    },
	{
      "src": [
        {
          "files": [ "Heleonix.Validation/bin/Release/netstandard2.0/Heleonix.Validation.dll" ],
          "src": "../../"
        }
      ],
      "dest": "api/Heleonix.Validation"
    },
	{
